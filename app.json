{
  "expo": {
    "name": "Heater",
    "slug": "Heater",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/manifest_1024.png",
    "splash": {
      "image": "./assets/manifest_1024.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/manifest_1024.png",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/manifest_1024.png"
    }
  }
}
